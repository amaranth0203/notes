set encoding=utf-8
let $LANG='zh_CN.UTF-8'
set guifont=Consolas:h14:b:cANSI
set guioptions-=m
set guioptions-=T


colo morning
set nocompatible
set expandtab
set backspace=indent,eol,start
set number
syntax on
set shiftwidth=4
set tabstop=4
set autochdir
map! <s-tab> <BS><BS><BS><BS>

set nobackup
set nowritebackup